void main() {
  int a = 10;
  int b = 20;

  // logical operators &&, ||, !

  print(a == 10 && b == 20);
  print(a != 10 && b == 20);
  print(a == 10 && b != 20);
  print(a != 10 && b != 20);

  print(a == 10 || b == 20);
  print(a != 10 || b == 20);
  print(a == 10 || b != 20);
  print(a != 10 || b != 20);
}
